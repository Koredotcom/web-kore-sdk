{
  "name": "kore-web-sdk",
  "version": "7.3.0",
  "description": "Kore.ai offers Bots SDKs as a set of platform-specific client libraries that provide a quick and convenient way to integrate Kore.ai Bots chat capability into custom applications",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-umd": "webpack --env=kore_env=prod --env=target_module=umd",
    "build": "webpack --env=kore_env=prod  --env=target_module=esm",
    "serve-umd": "webpack-dev-server --env=kore_env=dev  --env=component=chat  --env=target_module=umd",
    "serve": "webpack-dev-server --env=kore_env=dev --env=component=chat --env=target_module=esm"
  },
  "main": "dist/kore-web-sdk.esm.browser.js",
  "module": "dist/kore-web-sdk.esm.browser.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:RajasekharBa-Kore/web-kore-sdk.git#wsdk2"
  },
  "author": "Rajasekhar Balla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Koredotcom/web-kore-sdk/issues"
  },
  "homepage": "https://github.com/Koredotcom/web-kore-sdk#readme",
  "dependencies": {
    "@types/jquery": "^3.5.6",
    "d3": "^7.1.1",
    "express": "~4.9.0",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "jquery.tmpl": "0.0.2",
    "perfect-scrollbar": "^1.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "html-webpack-plugin": "^5.3.2",
    "imports-loader": "^3.0.0",
    "postcss": "^8.3.7",
    "postcss-css-variables": "^0.18.0",
    "postcss-loader": "^6.1.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  }
}
